{
    "env": {
      "node": true,
      "commonjs": true,
      "es2021": true
    },
    "extends": ["airbnb-base"],
    "parserOptions": {
      "ecmaVersion": 12
    },
    "rules": {
      "indent": "off",
      "linebreak-style": 0,
      "quotes": ["error", "double"],
      "semi": ["error", "always"],
      "no-irregular-whitespace": "off",
      "no-unused-vars": "off",
      "consistent-return": 0
    }
  }
  